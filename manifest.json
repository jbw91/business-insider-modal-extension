{
  "manifest_version": 2,
  "name": "Business Insider Fix",
  "description": "Get rid of that stupid modal if you've got an adblocker",
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.businessinsider.com/*"],
      "css": ["styles.css"]
    }
  ]
}